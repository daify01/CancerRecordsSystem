        </ObjectGroup>
        <ObjectGroup Name="connection" Order="2" Enabled="False">
          <Destination Path="" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=tcp:cancerrecprojectserver.database.windows.net,1433;Initial Catalog=TheCancerProject;User Id=mogundipe;Password=tiwatope8&amp;" includeData="False" />
